// My Graph
digraph {
	a [label=a color=red]
	b [label=b color=red]
	c [label=c color=red]
	d [label=d color=red]
	e [label=e color=red]
	f [label=f color=red]
		a -> b [label=2 dir=none]
		c -> a [label=5 dir=none]
		a -> d [label=1 dir=none]
		c -> b [label=3 dir=none]
		d -> b [label=2 dir=none]
		c -> d [label=3 dir=none]
		c -> e [label=1 dir=none]
		c -> f [label=5 dir=none]
		d -> e [label=1 dir=none]
		f -> e [label=2 dir=none]
}