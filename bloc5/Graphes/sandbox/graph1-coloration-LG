// My Graph
digraph {
	a [label=a color=blue]
	b [label=b color=red]
	c [label=c color=blue]
	d [label=d color=red]
	e [label=e color=red]
	f [label=f color=red]
	z [label=z color=red]
	x [label=x color=red]
	y [label=y color=blue]
		d -> a [dir=none]
		a -> z [dir=none]
		c -> b [dir=none]
		c -> d [dir=none]
		e -> c [dir=none]
		x -> y [dir=none]
}