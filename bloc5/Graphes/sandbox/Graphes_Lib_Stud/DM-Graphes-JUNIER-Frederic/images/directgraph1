// My Graph
digraph {
	a [label=a color=red]
	b [label=b color=red]
	c [label=c color=red]
	d [label=d color=red]
	e [label=e color=red]
	f [label=f color=red]
	g [label=g color=red]
	h [label=h color=red]
	i [label=i color=red]
		a -> h
		c -> a
		c -> b
		c -> e
		d -> b
		d -> c
		e -> h
		e -> f
		g -> e
		i -> e
}